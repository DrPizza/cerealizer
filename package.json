{
  "name": "cerealizer",
  "version": "0.0.1",
  "description": "JavaScript serialization library that can handle cycles and strives to preserve user-defined classes.",
  "main": "index.js",
  "engines": {
    "node": ">=11.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DrPizza/cerealizer"
  },
  "devDependencies": {
    "@types/node": "^11.11.3",
    "bson": "^4.0.2",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "deep-eql": "^4.0.0",
    "mocha": "^6.0.2",
    "should": "^13.2.3",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript\" -c \"cyan.bold\" \"npm run watch-ts\"",
    "watch-ts": "tsc -w",
    "test": "./node_modules/.bin/mocha --reporter spec"
  },
  "testling": {
    "harness": "mocha-bdd",
    "files": "test/*.js"
  },
  "keywords": [
    "serialization",
    "javascript"
  ],
  "author": "Peter Bright",
  "license": "MIT",
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  }
}
